{
	"name": "ts-react-starter",
	"version": "1.1.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "webpack-dev-server --open --hot --config webpack.dev.js",
		"dev": "webpack-dev-server --hot --inline --open",
		"build": "webpack --config webpack.prod.js",
		"test": "jest",
		"watch": "jest --watch",
		"compile": "tsc --watch",
		"lint": "eslint 'src/*/**/*.{js,ts,tsx}' --cache",
		"prettier": "prettier --check 'src/*/**/*.{js,ts,tsx}'",
		"release": "standard-version"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint 'src/*/**/*.{js,jsx,ts,tsx}' --cache",
			"prettier --write"
		],
		"*.{css,md}": "prettier --write"
	},
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.7.7",
		"@babel/plugin-proposal-optional-chaining": "^7.8.3",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.7.7",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"@types/jest": "^24.0.25",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@types/webpack-env": "^1.14.1",
		"@typescript-eslint/eslint-plugin": "^2.13.0",
		"@typescript-eslint/parser": "^2.13.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.4.0",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"file-loader": "^5.0.2",
		"html-webpack-plugin": "^3.2.0",
		"html-webpack-root-plugin": "^0.10.0",
		"husky": "^4.2.1",
		"image-webpack-loader": "^6.0.0",
		"jest": "^25.1.0",
		"jest-transform-stub": "^2.0.0",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"lint-staged": "^10.0.7",
		"prettier": "1.19.1",
		"react-hot-loader": "^4.12.18",
		"size-plugin": "^2.0.1",
		"source-map-loader": "^0.2.4",
		"speed-measure-webpack-plugin": "^1.3.1",
		"standard-version": "^7.1.0",
		"style-loader": "^1.1.2",
		"ts-jest": "^24.2.0",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.1",
		"tslint-immutable": "^6.0.1",
		"typescript": "^3.7.4",
		"webpack": "^4.41.5",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1",
		"webpack-merge": "^4.2.2"
	}
}
